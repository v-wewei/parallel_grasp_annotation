<mujoco model="parallel_jaw">
    <compiler autolimits="true"/>
    <asset>
        <mesh name="left_finger" file="../assets/hand_meshes/left_finger.obj"/>
        <mesh name="right_finger" file="../assets/hand_meshes/right_finger.obj"/>
        <mesh name="gripper_base" file="../assets/hand_meshes/gripper_base.obj"/>
    </asset>
    <actuator>
        <general dyntype="none" biastype="affine"  tendon="split1" forcerange="-800 800" ctrlrange="0 255" gainprm="0.1 0 0" biasprm="0 -1000 -100"/>
    </actuator>
    <worldbody>
        <body name="gripper_base" pos="0.0 0 0.00" quat="0.5 0.5 0.5 0.5">
            <geom pos="0 0 0." quat="1 0 0 0" type="mesh" mesh="gripper_base" density="5000" solref="0.01 1.1" solimp="1 1 0" group="1" name="hand_visual"/>

            <body name="left_finger" pos="-0.0255 0 0" quat="1 0 0 0">
                <joint name="finger_joint1" pos="0 0 0" axis="1 0 0" type="slide" limited="true" range="0.0 0.0255" armature="0.1" damping="1" />
                <geom type="mesh" mesh="left_finger" contype="0" conaffinity="0" group="1"  name="finger_l_visual" rgba="0.499 0.499 0.499 1" />
            </body>
            <body name="right_finger" pos="0.0255 0 0" quat="0. 0 0 1.0">
                <joint name="finger_joint2" pos="0 0 0" axis="1 0 0" type="slide" limited="true" range="0.0 0.0255" armature="0.1" damping="1"/>
                <geom quat="0 0 0 1" type="mesh" mesh="right_finger" contype="0" conaffinity="0" group="1"  name="finger2_visual" rgba="0.499 0.499 0.499 1" />
            </body>
        </body>
    </worldbody>
    <tendon>
        <fixed name="split1">
            <joint joint="finger_joint1" coef="0.5"/>
            <joint joint="finger_joint2" coef="0.5"/>
        </fixed>
    </tendon>
    <equality>
        <joint joint1="finger_joint1" joint2="finger_joint2" polycoef="0 1 0 0 0" solimp="0.95 0.99 0.001" solref="0.005 1"/>
    </equality>
</mujoco>
